{
  "name": "household-finance-vault",
  "version": "0.1.0",
  "description": "Secure, shareable household finance vault (prototype)",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --exec node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:renewal": "node scripts/test-renewal-models.js",
    "migrate": "node src/scripts/runMigrations.js up",
    "migrate:down": "node src/scripts/runMigrations.js down",
    "migrate:renewal": "node -e \"require('./src/utils/renewalMigration').runFullMigration({dryRun: false})\"",
    "migrate:renewal:dry": "node -e \"require('./src/utils/renewalMigration').runFullMigration({dryRun: true})\"",
    "lint": "echo \"Add linting scripts\" && exit 0",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fuzzball": "^2.2.3",
    "helmet": "^6.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf2json": "^3.2.0",
    "react-hook-form": "^7.64.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^5.2.2",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^2.0.22",
    "shadcn": "^3.2.1",
    "supertest": "^7.1.4"
  },
  "license": "MIT"
}
